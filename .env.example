# === Supabase Core ===
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_JWT_SECRET=super-secret-jwt-token-with-at-least-32-characters-long
SUPABASE_DB_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres
# SUPABASE_STUDIO_URL=http://127.0.0.1:54323
# SUPABASE_INBUCKET_URL=http://127.0.0.1:54324

# === Supabase Storage (S3 local) ===
S3_ACCESS_KEY=your-s3-access-key
S3_SECRET_KEY=your-s3-secret-key
S3_REGION=local
S3_HOST=127.0.0.1:54321/storage/v1/s3

# === Resend SMTP (custom email provider) ===
RESEND_API_KEY=your-resend-api-key-here
SMTP_ENABLED=true
SMTP_HOST=smtp.resend.com
SMTP_PORT=465
SMTP_USER=resend
SMTP_PASS=your-smtp-password-here # Same as API
SMTP_SENDER_EMAIL=onboarding@resend.dev
SMTP_SENDER_NAME=Onboarding

# === Optional: OpenAI key for Supabase Studio AI ===
OPENAI_API_KEY=your-openai-key-here
# SUPABASE_AUTH_SMS_TWILIO_AUTH_TOKEN=your-twilio-auth-token

# === App-specific ===
NEXT_PUBLIC_SITE_URL=http://127.0.0.1:3000

# === Redis (Local and Upstash) ===
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
UPSTASH_REDIS_REST_URL=""
UPSTASH_REDIS_REST_TOKEN=""
UPSTASH_REDIS_TCP_URL="redis://default:********@<endpoint>.upstash.io:<port>"

# === Feature Flags ===
FF_FULL_MAINTENANCE_MODE=false # Everything is off
FF_MAINTENANCE_MODE=false # Admin pages still on
FF_DISABLE_SIGNUP=false # No new signups
FF_NETWORKING_HUB_DISABLED=false
FF_ALUMNI_DIRECTORY_DISABLED=false
FF_SUCCESS_STORIES_DISABLED=false
FF_EVENTS_DISABLED=false